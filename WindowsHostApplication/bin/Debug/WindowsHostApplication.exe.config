      </serviceBehaviors>
    </behaviors>-->
    <client>
      <endpoint address="net.pipe://localhost/MovieName"
                binding="netNamedPipeBinding"
                contract="WindowsHostApplication.Contracts.IMovieName"/>
    </client>
  </system.serviceModel>
</configuration>